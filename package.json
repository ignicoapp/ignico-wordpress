        "border-top-right-radius",
        "border-bottom-right-radius",
        "border-bottom-left-radius",
        "border-image",
        "border-image-source",
        "border-image-slice",
        "border-image-width",
        "border-image-outset",
        "border-image-repeat",
        "outline",
        "outline-width",
        "outline-style",
        "outline-color",
        "outline-offset",
        "background",
        "background-color",
        "background-image",
        "background-repeat",
        "background-attachment",
        "background-position",
        "background-position-x",
        "background-position-y",
        "background-clip",
        "background-origin",
        "background-size",
        "box-decoration-break",
        "box-shadow",
        "text-shadow"
      ]
    }
  },
  "scripts": {
    "watch": "NODE_ENV=development gulp watch --require @babel/register --gulpfile tasks",
    "build:dev": "NODE_ENV=development gulp build --require @babel/register --gulpfile tasks",
    "build:prod": "NODE_ENV=production gulp build --require @babel/register --gulpfile tasks",
    "bump:prerelease": "NODE_ENV=production gulp bump --type prerelease --require @babel/register --gulpfile tasks",
    "bump:patch": "NODE_ENV=production gulp bump --type patch --require @babel/register --gulpfile tasks",
    "bump:minor": "NODE_ENV=production gulp bump --type minor --require @babel/register --gulpfile tasks",
    "bump:major": "NODE_ENV=production gulp bump --type major --require @babel/register --gulpfile tasks"
  }
}
